*
  margin: 0
  padding: 0
  border: 0

.backdrop
  background: #000
  border-radius: 5px
  min-width: 1200px
  padding: 50px 30px

.list
  list-style: none
  font-size: 0
  width: 100%
  display: inline-flex
  justify-content: space-between
  margin-bottom: 20px
  &:last-child
    justify-content: flex-start
    .item
      margin-right: 20px

.item
  display: inline-block
  list-style: none
  font-size: 0

.thumb
  display: inline-block
  border-radius: 5px
